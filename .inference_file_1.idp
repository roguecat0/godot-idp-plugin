vocabulary V {
	type redSlime := {28, 29}
	red_slime_hp : (redSlime) -> Int
	red_slime_aggro : (redSlime) -> Bool
	health_threshold : () -> Int

}
theory T:V {
	!slime in redSlime: red_slime_hp(slime) =< health_threshold() <=> ~red_slime_aggro(slime).

}
structure S:V {
	red_slime_hp := {28 -> 4, 29 -> 4}.
	health_threshold := 2.

}
procedure main() {
	pretty_print(model_expand(T,S,max=1))
}
