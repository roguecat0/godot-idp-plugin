vocabulary V {
	type country := {Belgium, Germany, France}
	type color := {Red, Blue, Green, Yellow}
	Bordering : (country * country) -> Bool
	ColourOf : (country) -> color
	countries_red : () -> Int

}
theory T:V {
	ColourOf(Belgium) = Red.
	#{ c in country: ColourOf(c) = Red } = countries_red().

}
structure S:V {
	Bordering := {(Belgium, France), (Belgium, Germany), (Germany, France)}.

}
procedure main() {
	pretty_print(maximize(T,S,term="countries_red()"))
}
