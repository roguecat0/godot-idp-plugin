vocabulary V {
	type GreenSlime := {}
	green_slime_hp : (GreenSlime) -> Int
	green_slime_aggro : (GreenSlime) -> Bool
	health_threshold : () -> Int

}
theory T:V {
	!slime in GreenSlime: green_slime_hp(slime) =< health_threshold() <=> ~green_slime_aggro(slime).

}
structure S:V {
	health_threshold := 2.

}
procedure main() {
	pretty_print(model_expand(T,S,max=1))
}
